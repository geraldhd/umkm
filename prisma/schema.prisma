generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  Id String @id @default(uuid())
  Email String @unique
  Pass String 
}

model Umkm_Pemasaran {
  Id String @id @default(uuid())
  Nomor Int @default(autoincrement())
  NamaBrand String @unique
  NamaOwner String
  Kategori String
  Alamat String
  NoHP String
  Keterangan String
  createdAt            DateTime    @default(now())
  updatedAt            DateTime    @updatedAt
}

model Umkm_Binaan {
  Id String @id @default(uuid())
  Nomor Int @default(autoincrement())
  NamaUMKM String @unique
  NamaPemilik String
  Kategori String
  Alamat String
  KabKota String
  NoHP String
  Keterangan String
  createdAt            DateTime    @default(now())
  updatedAt            DateTime    @updatedAt
}

model infoTanggal {
  Id String @id @default(uuid())
  Nomor Int @default(autoincrement())
  NamaProduk String
  NamaUMKM String
  NamaPemilik String
  JumlahBarang Int
  TanggalTerima DateTime
  TanggalExpired DateTime
  Status String
  createdAt            DateTime    @default(now())
  updatedAt            DateTime    @updatedAt
}



